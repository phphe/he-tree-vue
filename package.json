  "homepage": "https://he-tree-vue.phphe.com",
  "bugs": {
    "url": "https://github.com/phphe/he-tree-vue/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/phphe/he-tree-vue.git"
  },
  "scripts": {
    "postinstall": "node npm-scripts/postinstall.js",
    "dev": "vue-cli-service serve",
