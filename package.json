{
  "name": "he-tree-vue",
  "version": "3.1.2",
  "description": "A draggable sortable nested vue tree component.",
  "main": "dist/he-tree-vue.cjs.js",
  "module": "dist/he-tree-vue.esm.js",
  "types": "types/index.d.ts",
  "files": [
    "dist",
    "types",
    "npm-scripts"
  ],
  "unpkg": "dist/he-tree-vue.js",
